#include <stdio.h>

int main()
{
    int a;
    int r;
    int i;
    
    printf("Digite um n√∫mero");
    scanf("%d", &a);
    
    for(i=1; i<11; i++){
       r = a*i ;
       printf("%d",a);
       printf("x");
       printf("%d",i);
       printf("=");
       printf("%d\n",r);
    }
    

    return 0;
}
