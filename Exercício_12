#include <stdio.h>

int main()
{
    int a[6];
    int par = 0;
    int imp = 0;
    int i;
    
    for(i=1;i<6;i++){
        a[i] = i;
    }
    
    for(i=1;i<6;i++){
       if(a[i] % 2 == 0){
        printf("\n Par: %d", i);
        par++;
       }
       else{
        printf("\n Impar: %d", i);
        imp++;
       }
        
    }
    printf("\nQuantidade de pares: %d", par);
    printf("\nQuantidade de impares: %d", imp);
   
    return 0;
    
}
