#include <stdio.h>

int main()
{
    int d;
    int s;
    int cpf;
    float sub;
    float inss;
    float irrf;
    float des;
    float des2;
    
    
    printf("Digite seu CPF\n");
    scanf("%d", &cpf);
    printf("Digite a quantidade de dependentes que você possui: \n");
    scanf("%d", &d);
    printf("Digite sua renda mensal: \n");
    scanf("%d", &s);
    
    if(s < 1212.00){
        des = (s * 0.075);
    }
    else if (1212.01 < s || s < 2427.35){
        des = ((s * 0.09)-18.18);
    }
    else if (2427.36 < s || s < 3641.03){
        des = ((s * 0.12)-91);
    }
    else if (3641.04 < s || s < 7087.22){
        des = ((s * 0.14)-163.82);
    }
    else{
        des =  828.38;
    }
    
    inss = s - des;
    sub = inss - (d * 189.59);
    
    if (sub < 1903.98){
        irrf = inss;
    }
    else if(1903.99 < sub < 2826.65){
        irrf = inss - ((sub * 0.075) - 142.80);
    }
     else if(2826.66 < sub < 3751.05){
        irrf = inss - ((sub * 0.15) - 354.80);
    }
    else if(3751.06 < sub < 4664.68){
        irrf = inss - ((sub * 0.225) - 636.13);
    }
    else{
        irrf = inss - ((sub * 0.275) - 869.36);
    }
    
    printf("%f\n", des);
    printf("%f\n", sub);
    printf("%f\n", inss);
    printf("Seu sálario com desconto é: %f", irrf);

    return 0;
    
}
    
