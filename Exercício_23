#include <stdio.h>

int fibonacci(int x) {
    if(x==2 || x==1) {
        return 1;
    }
    return (fibonacci(x-2)+fibonacci(x-1));
}

int main()
{
    int y;
    printf("Digite o termo da sequÃªncia: ");
    scanf("%d", &y);
    
    for (int i = 1; i <= y; i++) {
    printf("%d ", fibonacci(i));
    }
    return 0;
}
